<main id="main">


    <!-- #featured-services -->
    <!--==========================
      Portfolio Section
    ============================-->
    <section id="portfolio" class="section-bg">
        <div class="container">

            <header class="section-header">
                <h3 class="section-title">Price & Plans</h3>
            </header>
            @if (Model.Count() > 0)
            {
                <div class="row portfolio-container">
                    @foreach (var item in Model)
                    {
                        <div class="col-lg-4 col-md-6 portfolio-item filter-hot wow fadeInUp" onclick="location.href='@Url.Action("Details", "Home", new { id = item.ProductID })'">
                            <div class="portfolio-wrap">
                                <figure>
                                    @if (item.ProductImages.Count() > 0)
                                    {
                                        <img src="data:image;base64,@System.Convert.ToBase64String(item.ProductImages.ElementAt(0).Image)" data-toggle="modal" data-target="#sharedModal" style="max-width:100%;max-height:100%" alt="">
                                    }
                                    else
                                    {
                                        <img src="~/img/office/Large Dedicated office.jpeg" data-toggle="modal" data-target="#sharedModal" style="max-width:100%;max-height:100%" alt="">
                                    }
                                </figure>
                                <div class="portfolio-info">
                                    <h4><a href="#" data-toggle="modal" data-target="#sharedModal">@item.ProductName</a></h4>
                                </div>
                            </div>
                        </div>
                    }
                </div>
            }
            else
            {
                <div class="row portfolio-container">
                    <div class="alert alert-success" id="msgalert">Plans are coming soon.</div>
                </div>
            }
        </div>

    </section>
